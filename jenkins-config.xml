<?xml version='1.0' encoding='UTF-8'?>
<project>
    <actions/>
    <description>CommitStage for einaralex/reference-tictactoe</description>
    <keepDependencies>false</keepDependencies>
    <properties>
        <com.coravy.hudson.plugins.github.GithubProjectProperty plugin="github@1.24.0">
            <projectUrl>https://github.com/einaralex/reference-tictactoe/</projectUrl>
            <displayName></displayName>
        </com.coravy.hudson.plugins.github.GithubProjectProperty>
    </properties>
    <scm class="hudson.plugins.git.GitSCM" plugin="git@3.0.1">
        <configVersion>2</configVersion>
        <userRemoteConfigs>
            <hudson.plugins.git.UserRemoteConfig>
                <url>https://github.com/einaralex/reference-tictactoe/</url>
            </hudson.plugins.git.UserRemoteConfig>
        </userRemoteConfigs>
        <branches>
            <hudson.plugins.git.BranchSpec>
                <name>*/master</name>
            </hudson.plugins.git.BranchSpec>
        </branches>
        <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
        <submoduleCfg class="list"/>
        <extensions/>
    </scm>
    <canRoam>true</canRoam>
    <disabled>false</disabled>
    <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
    <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
    <triggers>
        <hudson.triggers.TimerTrigger>
            <spec>00 08 * * *</spec>
        </hudson.triggers.TimerTrigger>
        <com.cloudbees.jenkins.GitHubPushTrigger plugin="github@1.24.0">
            <spec></spec>
        </com.cloudbees.jenkins.GitHubPushTrigger>
    </triggers>
    <concurrentBuild>false</concurrentBuild>
    <builders>
        <hudson.tasks.Shell>
            <command>
                sudo npm install -g nodemon
                sudo npm install -g create-react-app
                sudo npm install
                cd client
                sudo npm install
                cd ..
                make deploy
                echo Creating test report
                make testsonce</command>
        </hudson.tasks.Shell>
    </builders>
    <publishers>
        <hudson.tasks.BuildTrigger>
            <childProjects>Deployment</childProjects>
            <threshold>
                <name>SUCCESS</name>
                <ordinal>0</ordinal>
                <color>BLUE</color>
                <completeBuild>true</completeBuild>
            </threshold>
        </hudson.tasks.BuildTrigger>
        <hudson.tasks.junit.JUnitResultArchiver plugin="junit@1.19">
            <testResults>build/testOutput.xml</testResults>
            <keepLongStdio>false</keepLongStdio>
            <healthScaleFactor>1.0</healthScaleFactor>
            <allowEmptyResults>false</allowEmptyResults>
        <hudson.tasks.junit.JUnitResultArchiver plugin="junit@1.19">
            <testResults>build/testOutput.xml</testResults>
            <keepLongStdio>false</keepLongStdio>
            <healthScaleFactor>1.0</healthScaleFactor>
            <allowEmptyResults>false</allowEmptyResults>
        </hudson.tasks.junit.JUnitResultArchiver>
    </publishers>
    <buildWrappers/>
</project>

